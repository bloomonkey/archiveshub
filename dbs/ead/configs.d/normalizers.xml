    <subConfigs>

        <subConfig type="normalizer" id="identifierZeroStripNormalizer">
            <objectType>cheshire3.normalizer.RegexpNormalizer</objectType>
            <options>
                <setting type="regexp">^gb0+</setting>
                <setting type="char">gb</setting>
            </options>
        </subConfig>

    </subConfigs>
</config>