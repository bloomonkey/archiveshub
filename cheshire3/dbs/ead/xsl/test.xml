<div id="digitalobjectssection" class="section">
	<h3>
		Digital Objects
		<a id="daohelp" name="daohelp" target="_new"
			href="http://www.archiveshub.ac.uk/arch/images.shtml#dao-help">
			<img class="whatsthis" src="/images/whatisthissmall.gif"
				alt="[What is this?]"></a>
	</h3>
	<p>
		<b>Digital Object in archdesc/component</b>
	</p>
	<div class="daooptions" id="daooptnsdiv[1]">
		<input name="daooptns" value="new"
			onclick="createObjectsForm(1, '')" type="radio">
			Link to file
			<br>
				<input name="daooptns" value="embed"
					onclick="createObjectsForm(1, '')" checked="true" type="radio">
					Display image
					<br>
						<input name="daooptns" value="thumb"
							onclick="createObjectsForm(1, '')" type="radio">
							Display thumbnail link to file
							<br>
								<input name="daooptns" value="multiple"
									onclick="createObjectsForm(1, '')" type="radio">
									Link to multiple files
									<br>
	</div>
	<div class="embed" id="digitalobjectsform[1]">
		<table>
			<tbody>
				<tr>
					<td class="label">File URI:</td>
					<td>
						<input size="70" onfocus="setCurrent(this);"
							value="http://www.archiveshub.ac.uk/images/hublogo.gif"
							name="dao[1]/@href" id="dao[1]/@href" type="text"></td>
				</tr>
				<tr>
					<td class="label">Description:</td>
					<td>
						<input size="70" onfocus="setCurrent(this);"
							class="menuField" name="dao[1]/daodesc" id="dao[1]/daodesc"
							value="&lt;p&gt;&lt;/p&gt;" type="text"></td>
				</tr>
			</tbody>
		</table>
		<input value="embed" name="dao[1]/@show" type="hidden"></div>
