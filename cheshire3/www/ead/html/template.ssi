<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en">
  <head>
    <title>%REP_NAME% :: %TITLE%</title>
    
	<!-- screen CSS -->
	<link rel="stylesheet" type="text/css" media="screen" href="/ead/css/struc-all.css"></link>
	<link rel="stylesheet" type="text/css" href="/ead/css/style.css"></link>
    <!--[if lt IE 7]>
        <link rel="stylesheet" type="text/css" media="screen" href="/ead/css/ie6.css"></link>
        <script type="text/javascript">
            onload = function() { content.focus() }
        </script>
    <![endif]-->
    <link rel="stylesheet" type="text/css" href="/ead/css/localStyles.css"></link>
	
	<!-- printout CSS -->
	<link rel="stylesheet" type="text/css" media="print" href="/ead/css/print.css"></link>
		
	<!-- favourite icon -->
	<link rel="favourite icon" href="/images/c3.ico"></link>
	
	<script type="text/javascript" src="/ead/js/ead.js"></script>
